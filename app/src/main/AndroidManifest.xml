<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tarek360.instacapture_sample">

  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <activity android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>

    <activity
        android:name=".SimpleActivity"
        android:label="@string/sample_simple"
        android:theme="@style/AppTheme.NoActionBar"/>

    <activity
        android:name=".MapFragmentSampleActivity"
        android:label="@string/sample_google_map_fragment"/>
    <activity
        android:name=".MapViewSampleActivity"
        android:label="@string/sample_google_map_view"/>
    <activity
        android:name=".DialogSampleActivity"
        android:label="@string/sample_dialog"/>
    <activity
        android:name=".IgnoreViewsSampleActivity"
        android:label="@string/sample_ignore_views"/>

    <activity android:name=".ShowScreenShotActivity"/>

    <meta-data
        android:name="com.google.android.geo.API_KEY"
        android:value="AIzaSyACOgkkILLrGAmIpv_EPsv-6q2ZJ4cVL08"/>

  </application>

</manifest>